let three: nat_t =
  succ(succ(succ(zero)))

let add: (nat_t, nat_t) -> nat_t = {
  (n, k) => nat_ind(
    n, _ => nat_t,
    k, (prev, almost) => succ(almost))
}

@show three
@show add(three)
@show add(three, three)
